<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>me.dougal.conda-worker.plist</string>

        <key>ProgramArguments</key>
        <array>
            <string>/Users/vagrant/conda-worker.sh</string>
        </array>

        <key>StandardOutPath</key>
        <string>/Users/vagrant/.log/conda-worker-stdout</string>

        <key>StandardErrorPath</key>
        <string>/Users/vagrant/.log/conda-worker-stderr</string>

        <key>RunAtLoad</key>
        <true />
    </dict>
</plist>
